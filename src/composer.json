{
  "name": "duck-scraper/project",
  "description": "A web scraping project using PHP and Symfony components",
  "type": "project",
  "require": {
    "php": "^8.2",
    "phpoffice/phpword": "^1.0"
  },

  "autoload": {
    "psr-4": {
      "App\\": "."
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^5.3"
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "license": "MIT"
}
